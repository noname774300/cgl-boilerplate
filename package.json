    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-pwa": "^0.12.7"
  },
  "dependencies": {
    "crisp-game-lib": "^1.1.0"
  }
}
